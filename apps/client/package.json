{
  "name": "@xsolare/trip-scheduler-client",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "description": "Trip Scheduler Application",
  "author": {
    "name": "Ivan Kornilov",
    "email": "injurka@gmail.com"
  },
  "homepage": "https://trip-scheduler.ru",
  "main": "out/main/index.js",
  "engines": {
    "bun": ">=1.2.0"
  },
  "scripts": {
    "--------------------------------<      DEV       >--------------------------------": "",
    "dev": "vite --config build/vite.config.web.ts",
    "--------------------------------<      BUILD     >--------------------------------": "",
    "build": "vite build --config build/vite.config.web.ts",
    "icons:bundle": "bun run build/bundle-icons.ts",
    "preview": "vite preview --config build/vite.config.web.ts",
    "--------------------------------<   ELECTRON   >---------------------------------": "",
    "electron:dev": "electron-vite dev --config build/vite.config.electron.ts",
    "electron:build": "electron-vite build --config build/vite.config.electron.ts",
    "electron:build:linux:x64": "electron-vite build --config build/vite.config.electron.ts && electron-builder --linux --x64",
    "electron:build:win:x64": "electron-vite build --config build/vite.config.electron.ts && electron-builder --win --x64",
    "--------------------------------<   CAPACITOR   >---------------------------------": "",
    "cap:sync": "bun run build && bunx cap sync",
    "cap:open:android": "bunx cap open android",
    "cap:open:ios": "bunx cap open ios",
    "cap:run:android": "bunx cap run android",
    "cap:run:ios": "bunx cap run ios",
    "cap:asset": "capacitor-assets generate",
    "--------------------------------<   STORYBOOK   >---------------------------------": "",
    "storybook:dev": "storybook dev -p 6006",
    "storybook:build": "storybook build -o dist-storybook",
    "--------------------------------<   DATABASE    >---------------------------------": "",
    "db:seed": "TODO",
    "db:reset": "TODO",
    "--------------------------------< LINT & FORMAT >---------------------------------": "",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "vue-tsc --noEmit",
    "--------------------------------<     UTILS     >---------------------------------": "",
    "generate-pwa-assets": "pwa-assets-generator",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
